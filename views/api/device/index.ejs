<div class="page-heading">
  <h2>Devices</h2>      
</div>  

<table class="table table-hover table-bordered">
  <thead>
    <th><%= Object.keys(devices[0])[1] %></th>
    <th><%= Object.keys(devices[0])[0] %></th>
  </thead>
  <tbody>
    <% for(var i = 0; i < devices.length; i++) { %>
      <tr>
        <td><%= devices[i].id %></td>
        <td><%= devices[i].name %></td>
      </tr>
    <% } %>
  </tbody>
</table>

<ul class="pager">
  <li class="previous"><a href="/device/index?page=<%= previousPage %>">&larr; Older</a></li>
  <li class="next"><a href="/device/index?page=<%= nextPage %>">Newer &rarr;</a></li>
</ul>
  
